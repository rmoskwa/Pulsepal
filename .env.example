# PulsePal Environment Configuration
# Copy this file to .env and fill in your actual values

# Required: Google Gemini API Key
GOOGLE_API_KEY=your_gemini_api_key_here

# Required: Supabase Configuration
# Get these from your Supabase project dashboard
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_service_role_key_here

# Optional: LLM Model Selection
# Default is gemini-2.0-flash-exp
# LLM_MODEL=gemini-2.0-flash-exp

# Optional: Initialize embeddings on startup
# Set to "true" to pre-initialize embedding service
# INIT_EMBEDDINGS=false

# Optional: Conversation Logging (for debugging)
# Set to "true" to enable conversation logging
# ENABLE_CONVERSATION_LOGGING=false
# Directory to store conversation logs
# CONVERSATION_LOG_DIR=conversation_logs

# Optional: Session Configuration
# MAX_SESSION_DURATION_HOURS=24
# MAX_CONVERSATION_HISTORY=100

# Optional: RAG Configuration
# USE_HYBRID_SEARCH=true
# RAG_TOP_K=10
# RELEVANCE_THRESHOLD=0.7